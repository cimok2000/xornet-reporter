{
  "name": "reporter",
  "version": "0.0.21",
  "description": "The Xornet Reporter easily lets you connect your computer to the platform",
  "main": "index.js",
  "scripts": {
    "start": "nodemon .",
    "test": "mocha './test/*' --exit",
    "prettify": "npx prettier --write \"./**/*.js\"",
    "compile-linux": "pkg -t \"node14-linux-x64\" index.js -o \"xornet-reporter-linux-0.0.21.bin\" --compress Brotli",
    "compile-macos": "pkg -t \"node14-macos-x64\" index.js -o \"xornet-reporter-macos-0.0.21\" --compress Brotli",
    "compile-win": "pkg -t \"node14-win-x64\" index.js -o \"xornet-reporter-win-0.0.21\" --compress Brotli",
    "compress-linux": "bestzip build/xornet-reporter-linux-0.0.21.zip xornet-reporter-linux-0.0.21.bin lang",
    "compress-macos": "bestzip build/xornet-reporter-macos-0.0.21.zip xornet-reporter-macos-0.0.21 lang",
    "compress-win": "bestzip build/xornet-reporter-win-0.0.21.zip xornet-reporter-win-0.0.21.exe lang",
    "delete-binaries": "del xornet-reporter-win-0.0.21.exe && del xornet-reporter-macos-0.0.21 && del xornet-reporter-linux-0.0.21.bin",
    "build": "npm run prettify && npm run compile-linux && npm run compile-macos && npm run compile-win && npm run compress-linux && npm run compress-macos && npm run compress-win && npm run delete-binaries"
  },
  "keywords": [],
  "author": "Geoxor",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "colors": "^1.4.0",
    "os-locale": "^5.0.0",
    "progress": "^2.0.3",
    "simple-enum": "^1.0.0",
    "socket.io-client": "^4.0.2",
    "systeminformation": "^5.6.20"
  },
  "devDependencies": {
    "pkg": "^5.2.1",
    "bestzip": "^2.2.0",
    "chai": "^4.3.4",
    "mocha": "^8.4.0"
  }
}
